<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.imtiaj.tourmate01">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.EventListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.AddEventActivity" />
        <activity android:name=".Activity.CostListActivity"></activity>
        <activity android:name=".Activity.AddCostActivity" />

        <service
            android:name=".Service.MyIntentService"
            android:exported="false"></service>
    </application>

</manifest>